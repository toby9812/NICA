IF I196=0
GOTO *BARCODE2
ELSE
GOTO *D
ENDIF
*BARCODE2
SETM O176,1
WAITI I196
SETM O176,0
*E
MOVEX A=1,AC=0,SM=0,M1X,L,P215,R=V32%,H=1,MS	'P215
INH
LETVI V18%,215
*F
IF I195=0
GOTO *BARCODE1
ELSE
GOTO *F
ENDIF
*BARCODE1
SETM O175,1
WAITI I195
SETM O175,0
MOVEX A=1,AC=0,SM=0,M1X,L,P216,R=V32%,H=1,MS	'P216
INH
LETVI V18%,216
MOVEX A=1,AC=0,SM=0,M1X,L,P217,S= 100.0,H=1,MS	'P217
INH
